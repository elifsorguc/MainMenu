<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminControlLibrary">
    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="264dp"
        android:layout_height="235dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.559">

        <Button
            android:id="@+id/buttonBookAvailability"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Book Avaılabılıty"
            android:backgroundTint="@color/material_dynamic_tertiary70"/>

        <Button
            android:id="@+id/buttonStudyRoomAvailability"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Study Room Avaılabılıty"
            android:backgroundTint="@color/material_dynamic_tertiary70"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="306dp"
        android:layout_height="87dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.102">

        <TextView
            android:id="@+id/textViewController"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="CONTROL LIBRARY"
            android:textAlignment="center"
            android:textSize="34sp"
            android:textStyle="bold" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>